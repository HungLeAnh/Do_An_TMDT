    <script>
        $(".noreturnlink").click(function (e) {

            e.preventDefault();
            $.ajax({

                url: $(this).attr("href"), // comma here instead of semicolon
                success: function (data) {
                    $.notify(data.message, "success");               
                }

            });

        });



    </script>
    @RenderSection("Scripts", required: false)
